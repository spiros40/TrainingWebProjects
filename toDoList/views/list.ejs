<%- include("header"); %> 
<body>

    <div class="box" id="heading">
        <h1><%= listTitle %></h1>
    </div>

    <div class="box">        
            <% for(var i=0; i<newItemList.length; i++) { %>
                <div class="item">
                    <input type="checkbox">
                    <p><%= newItemList[i] %></p>
                </div>
            <% } %> 
            
            <form class="item" action="<%= postTo %> " method="post">
                <input type="text" name="newitem" placeholder="New item" autocomplete="off">
                <button type="submit" name="button">+</button>
            </form>
    </div>

    

  
<%- include("footer"); %> 

</body>
</html>